{
  "name": "lotto-subquery",
  "version": "1.0.0",
  "description": "SubQuery Indexer for the Lotto dApp",
  "main": "dist/index.js",
  "scripts": {
    "codegen-shibuya": "subql codegen -f project-shibuya.ts",
    "build-shibuya": "subql build -f project-shibuya.ts",
    "codegen-astar": "subql codegen -f project-astar.ts",
    "build-astar": "subql build -f project-astar.ts",
    "start:docker": "docker compose pull && docker compose up --remove-orphans",
    "dev": "subql codegen && subql build && docker compose pull && docker compose up --remove-orphans",
    "prepack": "rm -rf dist && npm run build",
    "test": "subql build && subql-node test"
  },
  "homepage": "https://github.com/GuiGou12358/lotto-subquery",
  "repository": "https://github.com/GuiGou12358/lotto-subquery",
  "files": [
    "dist",
    "schema.graphql",
    "project-shibuya.ts",
    "project-astar.ts"
  ],
  "author": "GuiGou",
  "license": "Apache-2.0",
  "resolutions": {
    "ipfs-unixfs": "6.0.6"
  },
  "dependencies": {
    "@subql/types-ethereum": "^3.13.1",
    "assert": "^2.1.0"
  },
  "devDependencies": {
    "@polkadot/api": "^12.4.2",
    "@subql/cli": "^5.2.7",
    "@subql/node": "^5.2.5",
    "@subql/node-ethereum": "^5.1.3",
    "@subql/substrate-wasm-processor": "^1.0.0",
    "@subql/testing": "^2.2.1",
    "@subql/types": "^3.11.2",
    "ethers": "^5.7.2",
    "typescript": "^5.6.2"
  },
  "exports": {
    "chaintypes": "src/chaintypes.ts",
    "index": "src/index.ts"
  }
}
